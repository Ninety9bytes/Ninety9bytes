{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "duff-and-phelps.famis.ui": {
      "root": "",
      "sourceRoot": ".",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "wwwroot",
            "index": "client-src/index.html",
            "main": "client-src/main.ts",
            "tsConfig": "client-src/tsconfig.app.json",
            "polyfills": "client-src/polyfills.ts",
            "assets": [
              "client-src/assets",
              "client-src/favicon.ico",
              "index.html"
            ],
            "styles": [
              "client-src/styles.scss",
              "node_modules/dragula/dist/dragula.css",
              "node_modules/ladda/css/ladda.scss",
              "node_modules/@progress/kendo-theme-bootstrap/dist/all.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/animate.css/animate.min.css"
            ],
            "scripts": [
              "node_modules/adal-angular/lib/adal.js",
              "node_modules/menuspy/dist/menuspy.min.js"
            ]
          },
          "configurations": {
            "prod": {
              "fileReplacements": [
                {
                  "replace": "client-src/environments/environment.ts",
                  "with": "client-src/environments/environment.prod.ts"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "client-src/environments/environment.ts",
                  "with": "client-src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "duff-and-phelps.famis.ui:build"
          },
          "configurations": {
            "qa": {
              "browserTarget": "duff-and-phelps.famis.ui:build:qa"
            },
            "prod": {
              "browserTarget": "duff-and-phelps.famis.ui:build:prod"
            },
            "production": {
              "browserTarget": "duff-and-phelps.famis.ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "duff-and-phelps.famis.ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "client-src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "client-src/polyfills.ts",
            "tsConfig": "client-src/tsconfig.spec.json",
            "scripts": [
              "node_modules/adal-angular/lib/adal.js",
              "node_modules/menuspy/dist/menuspy.min.js"
            ],
            "styles": [
              "client-src/styles.scss",
              "node_modules/dragula/dist/dragula.css",
              "node_modules/ladda/css/ladda.scss",
              "node_modules/@progress/kendo-theme-bootstrap/dist/all.css",
              "node_modules/angular2-busy/build/style/busy.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/animate.css/animate.min.css"
            ],
            "assets": [
              "client-src/assets",
              "client-src/favicon.ico",
              "index.html"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "duff-and-phelps.famis.ui-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "duff-and-phelps.famis.ui:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "duff-and-phelps.famis.ui",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": ""
    }
  }
}